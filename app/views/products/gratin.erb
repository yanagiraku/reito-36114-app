<% @product9.each do |product9| %>
  <div class='list'>
    <div class='product-img-content'>
      <%= image_tag product9.image, class: "product-img" %>
    </div>
    <div class='product-info'>
      <h3 class='product-name'>
        <%= product9.product_name %>
      </h3>
      <div class='product-company'>
        <span><%= product9.company.name %></span>
      </div>
    </div>
  </div>
<% end %>
